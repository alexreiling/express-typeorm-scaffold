    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
