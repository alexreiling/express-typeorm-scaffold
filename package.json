    "method-override": "^3.0.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "mongodb": "^3.6.0",
    "morgan": "^1.9.1",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.12",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
